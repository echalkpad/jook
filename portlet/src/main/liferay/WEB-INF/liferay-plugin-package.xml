<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin-package PUBLIC "-//Liferay//DTD Plugin Package 5.1.0//EN" "http://www.liferay.com/dtd/liferay-plugin-package_5_1_0.dtd">

<plugin-package>
	<name>${pom.name} for Liferay ${liferay.version}</name>
	<module-id>${groupId}/${artifactId}/${version}-${label}/${packaging}</module-id>
	<!-- <recommended-deployment-context>${artifactId}</recommended-deployment-context> -->
	<types>
		<type>portlet</type>
	</types>
	<short-description>
		${pom.description}
	</short-description>
	<change-log></change-log>
	<page-url>${pom.url}</page-url>
	<author>Railinc</author>
	<licenses>
		<license osi-approved="true">MIT</license>
	</licenses>
	<liferay-versions>
		<liferay-version>${liferay.version}+</liferay-version>
	</liferay-versions>
	
</plugin-package>